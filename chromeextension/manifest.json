{
  "manifest_version": 3,
  "name": "News Authenticity Checker",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "frontendExtension.html",
    "default_icon": "fakenewsIcon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
